<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="hospital">

	<insert id="enrollMember" parameterType="hospital">
		INSERT INTO HOSPITAL_MEMBER VALUES(SEQ_HOSPITAL_NO.NEXTVAL, #{id}, #{pw}, #{hospitalName}, #{hospitalNo}, #{managerName}, #{email}, #{phone}, default)
	</insert>


</mapper>
